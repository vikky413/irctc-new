        <div id="Dashboard" class="collpase">
            <div style="text-align: center;">
                <h1>Train</h1>

            </div>
        </div>

        <!-- ALL USERS LIST   -->

        <div id="addproduct" class="collpase">
            <div class="container">
                <div class="row">
                    <h1 style="color: white;">Total User Lists</h1>
                    <br>
                    <table class="table table-striped table-primary" style="background-color: white;">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Email Id</th>
                                <th>Phone Number</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% if(udata.length >0){
                                udata.forEach(function(row){
                                %>
                                <% if(row.email != "real@gmail.com"){ %>
                            <tr>
                                <td><%= row.fname %></td>
                                <td><%= row.email %></td>
                                <td><%= row.pnumber %></td>

                                <td>
                                    <a class="btn btn-danger btn-xs" href="/user/delete/<%= row._id %>"
                                        onclick="return confirm('Are you sure?')"><i class="fa fa-trash"></i></a>
                                </td>
                            </tr>
                            <% }})} %>
                        </tbody>
                    </table>


                </div>
            </div>
        </div>

        <!--ADD NEW Locations   -->

        <div id="account" class="collpase">
            <div class="container">
                <div class="row">

                    <div id="signupbox" style="margin-top:50px"
                        class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"> Add New stations</div>
                            </div>
                            <div class="panel-body">
                                <form action="/stations" id="EmployeeForm" class="form-horizontal" method="post"
                                    role="form">

                                    <div class="form-group">

                                        <label for="username" class="col-md-3 control-label">station Name</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="stn"
                                                placeholder="Enter Station Name">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <!-- Button -->
                                        <div class="col-md-offset-3 col-md-9">
                                            <input type="submit" name="submit" value="Add Station"
                                                class="btn btn-primary">
                                        </div>
                                    </div>


                                </form>
                            </div>
                        </div>




                    </div>
                </div>
            </div>
        </div>

        <!--ADD NEW CATEGORY LIST   -->

        <div id="userList" class="collpase">
            <div class="container">
                <div class="row">

                    <div id="signupbox" style="margin-top:50px"
                        class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"> Add New Trains</div>
                            </div>
                            <div class="panel-body">
                                <form action="/add-new-train" id="EmployeeForm" class="form-horizontal" method="post"
                                    role="form">

                                    <div class="form-group">
                                        <label for="username" class="col-md-3 control-label">Train Name</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="name" required
                                                placeholder="Enter Train Name">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="number" class="col-md-3 control-label">Train Number</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" maxlength="5" pattern="[0-9]{5}" name="number" required
                                                placeholder="Enter Train Number">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="number" class="col-md-3 control-label">Departure Time</label>
                                        <div class="col-md-9">
                                            <input type="time" class="form-control" maxlength="5" pattern="[0-9]{5}" name="dtime" required
                                                placeholder="Departure Time">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="number" class="col-md-3 control-label">Arrival Time</label>
                                        <div class="col-md-9">
                                            <input type="time" class="form-control" maxlength="5" pattern="[0-9]{5}" name="atime" required
                                                placeholder="Arrival Time">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <!-- Button -->
                                        <div class="col-md-offset-3 col-md-9">
                                            <input type="submit" name="submit" value="Add Train"
                                                class="btn btn-primary">
                                        </div>
                                    </div>


                                </form>
                            </div>
                        </div>




                    </div>
                </div>
            </div>
        </div>

        <!--  Total Payment Status  -->

