        function zoomImg() {

            console.log("zoomImgPopup", zoomImgPopup)
            zoomImgPopup.classList.add('open-zoomImgPopup')
            console.log("fffvfvfvc", imgVal)
            document.getElementById("myImg").src = imgVal;
